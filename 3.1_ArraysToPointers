//  arrays of pointers

#include <iostream>

using namespace std;

int main(){
    int a = 1, b = 990, c = 99; // Initialize three integers
    int *arr[3];                // Array of pointers to integers
    arr[0] = &a;
    arr[1] = &b;
    arr[2] = &c;
    
    cout << "arrays of pointers\n The Elements of array are: " << endl;
    
    for(int i = 0; i < 3; i++){
        cout << *arr[i] << " "; 
        // Print values pointed to by the pointers
    }
}
